{
	"title" : "IndexMe",
	"author" : "wayneliquan",
	"description" : "我的指引",
	"language": "zh-hans",
	"plugins": ["expandable-chapters-small"
		, "back-to-top-button"
		, "splitter"
		, "-lunr", "-search", "search-pro"
		, "hide-element"
		, "copy-code-button"
		, "insert-logo"
		, "custom-favicon"
		, "popup"
		, "editlink"
	],
	"pluginsConfig": {
  	"hide-element": {
    	"elements": [".gitbook-link"]
    }
		,"search-pro": {
        "cutWordLib": "nodejieba",
        "defineWord" : ["Gitbook Use"]
    }
		,"insert-logo": {
        "url": "/images/logo.png",
        "style": "background: none; max-height: 30px; min-height: 30px"
    }
		,"favicon": "./images/favicon.ico"
		,"editlink": {
				"base": "https://github.com/wayneliquan/IndexMe/blob/master/",
      	"label": "编辑本页"
    }
  }
}
